{
  "expo": {
    "name": "Eletrosom",
    "slug": "ReactEletro",
    "scheme": "reacteletro",
    "icon": "./Src/Components/assets/iconapp.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./Src/Components/assets/iconapp.png",
          "color": "#ffffff",
          "sounds": [
            "./local/path/to/myOtherSound.wav",
            "./local/path/to/myOtherSound.wav"
          ],
          "mode": "production"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ios.reacteletro"
    },
    "splash": {
      "image": "./Src/Components/assets/tela1.png",
      "backgroundColor": "#1534C8"
    },
    "android": {
      "package": "com.gabrielsaimo.ReactEletro"
    }
  }
}
